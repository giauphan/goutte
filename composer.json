{
    "name": "giauphan/goutte",
    "type": "application",
    "description": "A simple PHP Web Scraper",
    "keywords": ["scraper"],
    "homepage": "https://github.com/Giauphan/Goutte",
    "license": "MIT",
    "authors": [{
        "name": "Giau Phan",
        "email": "giauphan012@gmail.com"
    }],
    "require": {
        "php": ">=8",
        "symfony/deprecation-contracts": "^3.4",
        "symfony/browser-kit": "^7.0",
        "symfony/css-selector": "*",
        "symfony/dom-crawler": "*",
        "symfony/http-client": "*",
        "symfony/mime": "*"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.0"
    },
    "autoload": {
        "psr-4": { "Goutte\\": "Goutte" },
        "exclude-from-classmap": ["Goutte/Tests"]
    }
}
